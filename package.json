{
  "name": "jolocomwallet",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^22.0.0",
    "@types/node": "^10.0.6",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.55.7",
    "@types/react-native-material-ui": "^1.19.2",
    "@types/react-native-snap-carousel": "^3.6.0",
    "@types/react-native-sqlite-storage": "^3.3.0",
    "@types/react-native-vector-icons": "^4.6.0",
    "@types/react-navigation": "^1.2.3",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react-native": "^4.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "class-transformer": "^0.1.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "haul": "1.0.0-beta.14",
    "immutable": "^3.8.2",
    "jest": "^24.5.0",
    "material-colors": "^1.2.5",
    "mockdate": "^2.0.2",
    "prettier": "^1.15.3",
    "react-dom": "^16.3.0",
    "react-native-clean-project": "^3.0.1",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.13.1",
    "ts-jest": "^22.0.4",
    "ts-loader": "3.5.0",
    "tslib": "^1.7.1",
    "tslint": "^5.9.1",
    "typeorm": "^0.2.5",
    "typescript": "^3.0.1",
    "yarn": "^1.5.1"
  },
  "scripts": {
    "bundle:android": "haul start --platform android",
    "bundle:ios": "haul start --platform ios",
    "install:android": "cd android && ./gradlew installDebug",
    "install:ios": "echo 'Installing for ios not implemented yet.'",
    "run:android": "adb shell am start -n com.jolocomwallet/com.jolocomwallet.MainActivity",
    "run:ios": "echo 'Running for ios not implemented yet.'",
    "clean:node": "rm -rf node_modules",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:all": "react-native-clean-project",
    "build:ios": "haul bundle --entry-file='index.js' --bundle-output='./ios/smartwallet/main.jsbundle' --minify=false --dev=false --platform='ios' --assets-dest='./ios'",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "jest": {
    "setupFiles": [
      "./tests/utils/setup.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation|react-native-fabric)"
    ],
    "globals": {
      "window": true,
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/legacy/*"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.33",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.33",
    "@babel/preset-env": "^7.0.0-beta.33",
    "@babel/preset-react": "^7.0.0-beta.33",
    "@babel/register": "^7.0.0-beta.33",
    "@babel/runtime": "^7.0.0-beta.33",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-react-optimize": "^1.0.1",
    "bip39": "^2.5.0",
    "browser-ipfs": "^0.0.6",
    "crypto-js": "^3.1.9-1",
    "ethereumjs-wallet": "^0.6.0",
    "i18n-js": "^3.1.0",
    "jolocom-lib": "^2.3.0",
    "material-colors": "^1.2.5",
    "object.assign": "^4.1.0",
    "react": "16.8.3",
    "react-native": "0.59.1",
    "react-native-camera": "^1.1.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-indicator": "^0.7.0",
    "react-native-keychain": "^3.0.0-rc.3",
    "react-native-languages": "^3.0.1",
    "react-native-material-textfield": "^0.12.0",
    "react-native-material-ui": "^1.22.2",
    "react-native-md-textinput": "^2.0.4",
    "react-native-qrcode-scanner": "^1.0.1",
    "react-native-remote-svg": "^1.2.0",
    "react-native-snap-carousel": "^3.6.0",
    "react-native-splash-screen": "3.0.6",
    "react-native-sqlite-storage": "^3.3.4",
    "react-native-svg": "^6.3.1",
    "react-native-svg-uri": "^1.2.3",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.8",
    "react-navigation-redux-helpers": "^1.0.3",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sjcl": "^1.0.7",
    "sqlite3": "^4.0.6",
    "squel": "^5.12.1",
    "wif": "^2.0.6"
  },
  "resolutions": {
    "jolocom-lib/cred-types-jolocom-core": "0.0.10"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
